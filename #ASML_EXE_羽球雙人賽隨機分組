        "# STEP 1: Install packages (already satisfied in your case)\n",
        "!pip install --upgrade gspread gspread_dataframe\n",
        "\n",
        "# STEP 2: Authenticate in Colab\n",
        "from google.colab import auth\n",
        "auth.authenticate_user()\n",
        "\n",
        "import gspread\n",
        "import pandas as pd\n",
        "import random\n",
        "import google.auth\n",
        "\n",
        "# STEP 3: Authorize with google-auth\n",
        "creds, _ = google.auth.default()\n",
        "gc = gspread.authorize(creds)\n",
        "\n",
        "# STEP 4: Open Google Sheet\n",
        "sheet_url = \"https://docs.google.com/spreadsheets/d/1Ww90f1G0eZ0SQwCnWjd4nfVGS2uvB-XRmNmoY30oadk/edit?gid=0#gid=0\"\n",
        "spreadsheet = gc.open_by_url(sheet_url)\n",
        "worksheet = spreadsheet.sheet1  # Or use .worksheet(\"Your Tab Name\")\n",
        "\n",
        "# STEP 5: Read names from column A\n",
        "names = worksheet.col_values(1)\n",
        "names = [name.strip() for name in names if name.strip()]\n",
        "\n",
        "# STEP 6: Shuffle and pair\n",
        "random.shuffle(names)\n",
        "\n",
        "pairs = []\n",
        "for i in range(0, len(names) - 1, 2):\n",
        "    pairs.append((names[i], names[i+1]))\n",
        "\n",
        "if len(names) % 2 == 1:\n",
        "    pairs[-1] = (*pairs[-1], names[-1])  # last group is trio\n",
        "\n",
        "# STEP 7: Print result\n",
        "for i, group in enumerate(pairs, 1):\n",
        "    print(f\"Group {i}: {group}\")\n"
      ],
